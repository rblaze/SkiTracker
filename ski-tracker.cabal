name:           ski-tracker
version:        0.1
cabal-version:  >= 1.2
build-type:     Simple

executable ski-tracker
  hs-source-dirs:  src
  main-is:         Main.hs
  build-depends:   
                   base >= 4,
                   xml,
                   bytestring,
                   time,
                   old-locale
  ghc-options:     -Wall
  other-modules:   
                   Track,
                   TCX

